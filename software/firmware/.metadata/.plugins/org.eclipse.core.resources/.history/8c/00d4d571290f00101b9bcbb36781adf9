uint16_t IRsensorValues[16] = {0};



void analogReadIRs(void){
	for (uint8_t i = 10; i < 15; i++)
	    {
	        sensorValues[i] = AnalogMux_ReadChannel(i);
	    }
}


uint16_t readRawIR(IR ir)
{
	switch(ir){
	case IR_FRONT:
		break;
	case IR_LEFT_BACK:
		break;
	case IR_LEFT_FORWARD:
		break;
	case IR_RIGHT_BACK:
		break;
	case IR_RIGHT_FORWARD:
		break;
	}
}

