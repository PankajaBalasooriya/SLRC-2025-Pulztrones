#ifndef INC_IRS_H_
#define INC_IRS_H_

#include "sensors.h"

typedef enum
{
	IR_LEFT_FORWARD = 0,
	IR_LEFT_BACK = 1,
	IR_FRONT = 2,
	IR_RIGHT_FORWARD = 3,
	IR_RIGHT_BACK = 4
}IR;


void analogReadIRs(void);

uint16_t readRawIR(IR ir);


#endif /* INC_IRS_H_ */
